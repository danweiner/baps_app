<%= simple_form_for(owner) do |f| %>  
  <%= f.input :name %>
  <%= f.input :city %>
  <%= f.input :country %>
  <%= f.input :description, label: 'About Me' %> 
  <%= f.input :attachment, as: :file, label: "File" %>
  <%= f.input :attachment_cache, label: "Attached Files" %>

  <%= f.button :submit, "Submit", class: "btn-primary"%>
<% end %>